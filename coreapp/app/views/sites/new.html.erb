<h1>New site</h1>

<% form_for(@site) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :url %><br />
    <%= f.text_field :url %>
  </p>
  <p>
    <%= f.hidden_field :account_id, :value => current_account.id %>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', root_url %>
