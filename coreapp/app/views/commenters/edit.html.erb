<h1>Editing commenter</h1>

<% form_for [@page, @commenter] do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', page_commenter_path(@page, @commenter) %> |
<%= link_to 'Back', page_commenters_path(@page) %>
