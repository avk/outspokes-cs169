<%- if logged_in? -%>
  <%= h(current_account.email) %>
  ( <%= link_to "log out", logout_path, { :title => "Log out" } %> )
  <hr />
  
  <%= link_to "Account", edit_account_path(current_account.id) %>
  -
  <%= link_to "Dashboard", dashboard_account_path(current_account.id) %>
<%- else -%>
  <%= link_to "Log in",  login_path,  { :title => "Log in" }  %>
  |
  <%= link_to "Sign up", signup_path, { :title => "Create an account" } %>
<% end -%>
