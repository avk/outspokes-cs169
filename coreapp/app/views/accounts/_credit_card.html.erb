<table id="credit_card_form">
  <tbody><tr>
      <td>
        <label for="card_number">Card number</label>
      </td>
      <td>
        <input type="text" size="17" name="ccnumber" maxlength="16" id="card_number" class="transaction_field"/>
      </td>
      <td>
        <!--<img width="17x21" align="absmiddle" src="/images/marketing/icon-lock.png?1252091147" alt="Secure"/><strong style="color: rgb(0, 0, 0);">Secure</strong>-->
      </td>
    </tr>
    <tr>
      <td>
        <label for="card_expires_on_month">Expires on</label>
      </td>
      <td colspan="2">
        <select name="card_expires_on[month]" id="card_expires_on_month" class="transaction_field">
          <option value="1">1 - January</option>
          <option value="2">2 - February</option>
          <option value="3">3 - March</option>
          <option value="4">4 - April</option>
          <option value="5">5 - May</option>
          <option value="6">6 - June</option>
          <option value="7">7 - July</option>
          <option value="8">8 - August</option>
          <option value="9" selected="selected">9 - September</option>
          <option value="10">10 - October</option>
          <option value="11">11 - November</option>
          <option value="12">12 - December</option>
        </select>
        <select name="card_expires_on[year]" id="card_expires_on_year" class="transaction_field">
          <option value="2009" selected="selected">2009</option>
          <option value="2010">2010</option>
          <option value="2011">2011</option>
          <option value="2012">2012</option>
          <option value="2013">2013</option>
          <option value="2014">2014</option>
          <option value="2015">2015</option>
          <option value="2016">2016</option>
          <option value="2017">2017</option>
          <option value="2018">2018</option>
          <option value="2019">2019</option>
          <option value="2020">2020</option>
          <option value="2021">2021</option>
          <option value="2022">2022</option>
          <option value="2023">2023</option>
          <option value="2024">2024</option>
        </select>
        <input type="hidden" name="ccexp" id="card_expires_on" class="transaction_field"/>
      </td>
    </tr>
    <tr>
      <td>
        <label for="card_zip_code">Billing ZIP</label>
      </td>
      <td colspan="2">
        <input type="text" size="11" name="postal_code" maxlength="10" id="card_zip_code" class="transaction_field"/>
      </td>
    </tr>
<!--    <tr>
      <td>
      </td>
      <td valign="top" colspan="2">
        (or Postal Code if not in the USA)
      </td>
    </tr>-->
  </tbody></table>
