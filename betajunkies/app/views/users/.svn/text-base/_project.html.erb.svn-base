
  <tr>
    <td><%= link_to project.name, project %></td>
    <td><%= link_to project.user.login, :controller => "users", :action => "view", :id => project.user.id %></td>
    <td><%=h project.summary %>
    	<blockquote>
    	<% if @current_user == project.user %>
    		<%= link_to 'Edit Project', edit_project_path(project) %> | 
    		<%= link_to 'Delete Project', project, :confirm => 'Are you sure?', :method => :delete %>
    	<% end %>
    	</blockquote>
    </td>
    <td>
	<% if project.ratings.average('value') != nil %> 
	      <%= project.ratings.average('value').truncate(1) %> </p>
            <% else %>
	      Unrated
	    <% end %>
    </td>
    <td><a href="http://<%=h project.link %>"><%=h project.link %></a></td>
  </tr>
